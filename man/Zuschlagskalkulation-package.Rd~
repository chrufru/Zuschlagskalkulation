\examples{
# Mit einer Kostenstelle
Zuschl.kalk.handwerk(300,140,120,0)
# Mit mehreren Kostenstellen
Zuschl.kalk.industrie(NA,0,120,0,40,0,20394,2004,0,20483,20)
}
